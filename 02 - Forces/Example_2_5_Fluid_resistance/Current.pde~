

class Liquid
{

  /*
    Liquids are rectangular areas, and know about their own
    x and y coords, and width and height. */
  float x,y,w,h;
  // coefficient of drag
  float c;

  Liquid(float x_, float y_, float w_, float h_, float c_) {
    x = x_;
    y = y_;
    w = w_;
    h = h_;
    c = c_;
  }

  void display() {
    noStroke();
    fill(175);
    rect(x,y,w,h);
  }

}
